{
  "name": "es6-crawler-detect",
  "version": "3.2.0",
  "description": "This is an ES6 adaptation of the original PHP library CrawlerDetect, this library will help you detect bots/crawlers/spiders vie the useragent.",
  "main": "src/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js",
    "dev": "cross-env NODE_ENV=development webpack --progress --config webpack.dev.js",
    "test": "mocha test/**/*.test.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --ext .js --ignore-path .eslintignore . --fix",
    "npm:minor": "npm version minor",
    "npm:major": "npm version major",
    "npm:patch": "npm version patch",
    "npm:publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JefferyHus/es6-crawler-detect.git"
  },
  "keywords": [
    "es6-javascript",
    "crawler",
    "bots",
    "spider",
    "detection"
  ],
  "author": "Jeffery Hussin <jefferytutorials@gmail.com> (http://github.com/jefferyhus)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JefferyHus/es6-crawler-detect/issues"
  },
  "homepage": "https://github.com/JefferyHus/es6-crawler-detect#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^8.2.3",
    "core-js": "^3.20.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
