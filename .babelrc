{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					// The % refers to the global coverage of users from browserslist
					"browsers": ["> 0.25%, not dead"]
				},
				"useBuiltIns": "usage",
				"corejs": { "version": "3", "proposals": true }
			}
		]
  ],
  "sourceType": "unambiguous",
	"plugins": [
		"@babel/plugin-proposal-class-properties"
	]
}